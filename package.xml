<?xml version="1.0"?>
<package>
  <name>voxel3d_nodelet</name>
  <version>2.0.0</version>
  <description>ROS nodelet for 5Voxel 5Z01A ToF camera</description>

  <maintainer email="jackie.lee@5voxel.com">Jackie Lee</maintainer>

  <license>BSD</license>

  <url type="website">https://github.com/5VOXEL/voxel3d_ros</url>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>nodelet</build_depend>
  <build_depend>image_transport</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>std_msgs</build_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>nodelet</run_depend>
  <run_depend>image_transport</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>std_msgs</run_depend>

  <export>
    <nodelet plugin="${prefix}/voxel3d_nodelet.xml" />
  </export>
</package>
